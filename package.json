{
  "name": "rainman",
  "version": "1.0.0",
  "description": "React-Alt-Express starter",
  "main": "app/server/server.js",
  "scripts": {
    "react-test": "jest",
    "test": "./node_modules/.bin/mocha",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel app -d lib --experimental -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "dev-start": "npm run watch-js & npm run dev-server & npm run server",
    "start": "npm run build && node lib/server/server",
    "build": "./node_modules/.bin/babel app -d lib --experimental && npm run webpack",
    "webpack": "./node_modules/.bin/webpack lib/client/render.js public/js/app.js",
    "migrate": "npm run build && node migration.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ]
  },
  "author": "joeyq",
  "license": "ISC",
  "dependencies": {
    "alt": "^0.17.3",
    "axios": "^0.5.4",
    "babel": "^5.8.23",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.13.3",
    "bookshelf": "^0.8.2",
    "cookie-parser": "^1.3.5",
    "express": "^4.13.3",
    "express-flash": "0.0.2",
    "express-session": "^1.11.3",
    "immutable": "^3.7.5",
    "iso": "^4.2.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^5.0.5",
    "knex": "^0.8.6",
    "lodash": "^3.10.1",
    "mongoose": "^4.1.5",
    "morgan": "^1.6.1",
    "mysql": "^2.9.0",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.25.1",
    "react-router": "*",
    "webpack": "^1.12.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "jest-cli": "^0.5.4",
    "react-tools": "*",
    "mocha": "^2.3.0",
    "nodemon": "^1.4.1",
    "react-hot-loader": "^1.3.0",
    "rewire": "^2.3.4",
    "sinon": "^1.16.1",
    "supertest": "^1.1.0",
    "webpack-dev-server": "^1.10.1"
  }
}
